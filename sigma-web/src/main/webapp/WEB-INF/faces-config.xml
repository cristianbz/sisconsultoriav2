<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<application>
		<resource-bundle>
			<base-name>textos</base-name>
			<var>txt</var>
		</resource-bundle>
		<locale-config>
			<default-locale>es_ES</default-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>sis.etiquetas</base-name>
			<var>etiquetas</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>sis.titulos</base-name>
			<var>titulos</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>sis.botones</base-name>
			<var>botones</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>sis.mensajes</base-name>
			<var>mensajes</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>sis.tooltip</base-name>
			<var>tooltip</var>
		</resource-bundle>
		
		
	</application>
	<factory>
		<exception-handler-factory>ec.gob.ambiente.sigma.web.utils.ViewExpiredExceptionExceptionHandlerFactory</exception-handler-factory>
	</factory>
</faces-config>
